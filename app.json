{
  "expo": {
    "name": "MAPeando",
    "slug": "MAPeando",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/mapeando_logo.png",
    "splash": {
      "image": "./assets/mapeando_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#450920"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/mapeando_logo.png",
        "backgroundColor": "#450920"
      },
      "package": "com.alejo_paoloni.MAPeando"
    },
    "web": {
      "favicon": "./assets/mapeando_logo.png"
    }
  }
}
